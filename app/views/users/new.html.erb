<h1> Sign Up </h1>

<%= form_for :user do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :user_name %><br>
    <%= f.text_field :user_name %>
  </p>
   <p>
    <%= f.label :password %><br>
    <%= f.text_field :password %>
  </p>
  <p>
    <%= f.label :password_confirm %><br>
    <%= f.text_field :password_confirm %>
  </p>
  <p>
    <%= f.submit %>
  </p>
<% end %>
